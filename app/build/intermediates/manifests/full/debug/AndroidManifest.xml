        android:minSdkVersion="23"
        android:targetSdkVersion="27" />

    <uses-permission android:name="android.permission.SEND_SMS" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.desagil.grupo.ponto.fabioferrao.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.desagil.grupo.ponto.fabioferrao.smsToNumber" />
        <activity android:name="com.desagil.grupo.ponto.fabioferrao.Morse" />
        <activity android:name="com.desagil.grupo.ponto.fabioferrao.careScreen" />
    </application>

</manifest>